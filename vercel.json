{
	"version": 2,
	"builds": [
		{ "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "build", "installCommand": "npm ci", "buildCommand": "npm run build" } }
	],
	"routes": [
		{ "handle": "filesystem" },
		{ "src": "/(.*)", "dest": "build/$1" },
		{ "src": "/.*", "dest": "build/index.html" }
	]
}


